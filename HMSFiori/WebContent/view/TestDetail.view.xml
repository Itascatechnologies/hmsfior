<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
xmlns:l="sap.ui.layout"
  xmlns:f="sap.ui.layout.form"

		controllerName="test.my.fiori.view.TestDetail" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{i18n>testDetailPageTitle}"  id="idDetailPage"  showHeader="{device&gt;/isPhone}"  showNavButton="{device&gt;/isPhone}" navButtonPress="navToEmptyViewForWindowCase">
	<footer>
      <Bar>
        <contentRight>
           <Button id="delete" text="Delete" press="handleDeletePress" />
          <Button id="edit" text="Edit" press="handleEditPress" />
          <Button id="save" text="Save" type="Emphasized" visible="false" press="handleSavePress" />
          <Button id="cancel" text="Cancel" visible="false" press="handleCancelPress" />
        </contentRight>
      </Bar>
    </footer>	
  <customData><core:CustomData key="sapDtResourcePath" value="TestMasters"/></customData></Page>
</core:View>